<script src="/js/dashboard.js"></script>

<h1>Dashboard</h1>
{{#each posts as |post|}}
  <div class="post" onclick="onClickPost({{post.id}})">
    <p>{{post.title}}</p>
    <p>{{post.contents}}</p>
    <p>{{post.userName}}</p>
    <p>{{post.date}}</p>
  </div>
{{/each}}
<button class="btn" id="createPostButton" onclick="addNewPost()">+ New Post</button>